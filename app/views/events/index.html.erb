<h1>Listing events</h1>

<table>
  <tr>
    <th>Web property</th>
    <th>Page</th>
    <th>Referrer</th>
    <th>Browser time</th>
    <th>Data</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.web_property.name %></td>
    <td><%= event.page.url %></td>
    <td><%= event.referrer.url %></td>
    <td><%= event.browser_time %></td>
    <td><%= event.data.to_json %></td>
    <td><%= link_to 'Show', event %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event', new_event_path %>
